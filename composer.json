{
    "name": "bdanzer/dp-plugin",
    "type": "wordpress-plugin",
    "repositories": [
        {
        "type": "composer",
        "url": "https://wpackagist.org"
        },
        {
            "type": "vcs",
            "url": "git@github.com:bdanzer/danzerpress-plugin.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Danzerpress\\": [
                "dp-sections", 
                "dp-sections/contexts", 
                "dp-sections/sections", 
                "dp-sections/helpers", 
                "inc"
            ],
            "Danzerpress\\filters\\" : [
                "filters"
            ]
        }
    }
}